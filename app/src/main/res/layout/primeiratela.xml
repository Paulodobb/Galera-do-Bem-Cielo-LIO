<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#616DA6">

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="wrap_content"
        android:layout_height="134dp"

        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_weight="0.35"
        android:background="@drawable/capa"
        android:orientation="horizontal"
        tools:ignore="ObsoleteLayoutParam"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"></RelativeLayout>

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="180dp">

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/screen_border"
            android:text="Comprar"
            android:textAllCaps="false"
            app:layout_constraintLeft_toLeftOf="@+id/relativeLayout2"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout2"
            tools:ignore="HardcodedText"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1" />

        <Button
            android:id="@+id/tapiocaBTid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:layout_marginTop="124dp"
            android:background="@drawable/screen_border"
            android:text="Doar R$20"
            android:textAllCaps="false"
            app:layout_constraintLeft_toLeftOf="@+id/relativeLayout8"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1" />

        <Button
            android:id="@+id/acaiBTid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/screen_border"
            android:text="Doar R$10"
            android:textAllCaps="false"
            app:layout_constraintLeft_toLeftOf="@+id/relativeLayout6"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout6"
            tools:ignore="HardcodedText"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1" />

        <Button
            android:id="@+id/amendoimBTid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/screen_border"
            android:text="Doar R$5"
            android:textAllCaps="false"
            app:layout_constraintLeft_toLeftOf="@+id/relativeLayout3"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout3"
            tools:ignore="HardcodedText"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1" />

        <Button
            android:id="@+id/chocolateBTid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/screen_border"
            android:text="Doar R$30"
            android:textAllCaps="false"
            app:layout_constraintLeft_toLeftOf="@+id/relativeLayout5"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout5"
            tools:ignore="HardcodedText"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1" />

        <Button
            android:id="@+id/morangoBTid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/screen_border"
            android:text="Doar R$100"
            android:textAllCaps="false"
            app:layout_constraintLeft_toLeftOf="@+id/relativeLayout7"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout7"
            tools:ignore="HardcodedText"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1" />

        <Button
            android:id="@+id/cocoBTid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/screen_border"
            android:text="Doar R$50"
            android:textAllCaps="false"
            app:layout_constraintLeft_toLeftOf="@+id/relativeLayout2"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout2"
            tools:ignore="HardcodedText"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginLeft="6dp" />

        <RelativeLayout
            android:id="@+id/relativeLayout3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/cinco"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="@+id/relativeLayout6"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/relativeLayout5"
            app:layout_constraintRight_toRightOf="@+id/relativeLayout5"
            app:layout_constraintTop_toTopOf="@+id/relativeLayout6"
            app:layout_constraintVertical_bias="1.0"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"></RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout8"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="14dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/vinte"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/tapiocaBTid"
            app:layout_constraintLeft_toRightOf="@+id/relativeLayout6"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1">

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout5"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="14dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/trinta"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="@+id/relativeLayout2"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/relativeLayout2"
            app:layout_constraintTop_toTopOf="@+id/relativeLayout2"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1">

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout2"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_marginTop="84dp"
            android:background="@drawable/cinquenta"
            android:orientation="horizontal"
            app:layout_constraintLeft_toLeftOf="@+id/relativeLayout6"
            app:layout_constraintRight_toRightOf="@+id/relativeLayout6"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout6"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"></RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout6"
            android:layout_width="100dp"
            android:layout_height="0dp"
            android:background="@drawable/dez"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="@+id/relativeLayout8"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/relativeLayout8"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"></RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout7"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_marginTop="84dp"
            android:background="@drawable/cem"
            android:orientation="horizontal"
            app:layout_constraintLeft_toLeftOf="@+id/relativeLayout8"
            app:layout_constraintRight_toRightOf="@+id/relativeLayout8"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout8"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"></RelativeLayout>
    </android.support.constraint.ConstraintLayout>

    <ImageView
        android:id="@+id/a1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/relativeLayout"
        android:layout_alignParentStart="true"
        android:layout_marginStart="36dp"
        app:srcCompat="?android:attr/selectableItemBackground"
        android:contentDescription=""
        tools:ignore="ContentDescription" />

</RelativeLayout>